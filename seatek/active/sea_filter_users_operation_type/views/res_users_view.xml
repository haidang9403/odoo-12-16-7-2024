<?xml version="1.0"?>
<odoo>
    <record id="view_users_operation_type_form" model="ir.ui.view">
        <field name="name">sea.users.operation.type.form</field>
        <field name="model">res.users</field>
        <field name="inherit_id" ref="base.view_users_form"/>
        <field name="arch" type="xml">
            <notebook>
                <page string="Sea Filter">
                    <group>
                        <group>
                            <field name="sea_user_operation_type_ids" options="{'no_create': True}" widget="many2many_tags"/>
                        </group>
                        <group>
                            <field name="sea_user_warehouse_ids" options="{'no_create': True}" widget="many2many_tags"/>
                        </group>
                    </group>
                </page>
            </notebook>
        </field>
    </record>
</odoo>

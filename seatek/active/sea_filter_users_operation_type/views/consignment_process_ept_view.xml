<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="consignment_transfer_warehouse_form" model="ir.ui.view">
        <field name="name">consignment.transfer.warehouse.form</field>
        <field name="model">consignment.process.ept</field>
        <field name="inherit_id" ref="consignment_management_ept.form_view_consignment_transfer_ept"/>
        <field name="arch" type="xml">
            <field name="warehouse_id" position="replace">
                <field name="warehouse_id" options="{'no_create': True}"/>
            </field>
        </field>
    </record>
    <record id="consignment_return_warehouse_form" model="ir.ui.view">
        <field name="name">consignment.return.warehouse.form</field>
        <field name="model">consignment.process.ept</field>
        <field name="inherit_id" ref="consignment_management_ept.form_view_consignment_transfer_return_ept"/>
        <field name="arch" type="xml">
            <field name="warehouse_id" position="replace">
                <field name="warehouse_id" options="{'no_create': True}"/>
            </field>
        </field>
    </record>
</odoo>
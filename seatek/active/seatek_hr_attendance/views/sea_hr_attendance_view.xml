<?xml version="1.0" encoding="UTF-8"?>
<odoo>
    <record id="sea_hr_attendance_view_search" model="ir.ui.view">
        <field name="name">sea.hr.attendance.search</field>
        <field name="model">sea.hr.attendance</field>
        <field name="arch" type="xml">
            <search>
                <field name="company_id"/>
                <field name="month"/>
                <field name="year"/>
                <separator/>
                <filter string="Tháng 1" domain="[('month', '=', 1)]" name="Jan"/>
                <filter string="Tháng 2" domain="[('month', '=', 2)]" name="Feb"/>
                <filter string="Tháng 3" domain="[('month', '=', 3)]" name="Mar"/>
                <filter string="Tháng 4" domain="[('month', '=', 4)]" name="Apr"/>
                <filter string="Tháng 5" domain="[('month', '=', 5)]" name="May"/>
                <filter string="Tháng 6" domain="[('month', '=', 6)]" name="Jun"/>
                <filter string="Tháng 7" domain="[('month', '=', 7)]" name="Jul"/>
                <filter string="Tháng 8" domain="[('month', '=', 8)]" name="Aug"/>
                <filter string="Tháng 9" domain="[('month', '=', 9)]" name="Sep"/>
                <filter string="Tháng 10" domain="[('month', '=', 10)]" name="Oct"/>
                <filter string="Tháng 11" domain="[('month', '=', 11)]" name="Nov"/>
                <filter string="Tháng 12" domain="[('month', '=', 12)]" name="Dec"/>
                <separator/>
                <filter string="Năm 2023" domain="[('year', '=', 2023)]" name="Year"/>
                <filter string="Năm 2024" domain="[('year', '=', 2024)]" name="Year"/>
                <filter string="Năm 2025" domain="[('year', '=', 2025)]" name="Year"/>
                <group expand='0' string='Group by...'>
                    <filter string='Công ty' domain="[]" name="company_id_group" context="{'group_by': 'company_id'}"/>
                    <filter string='Tháng' domain="[]" name="month_group" context="{'group_by': 'month'}"/>
                    <filter string='Năm' domain="[]" name="year_group" context="{'group_by' : 'year'}"/>
                </group>
            </search>
        </field>
    </record>

    <record id="sea_hr_attendance_action" model="ir.actions.act_window">
        <field name="name">Attendances</field>
        <field name="res_model">sea.hr.attendance</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[('company_id','in',company_ids)]</field>
        <field name="view_id" eval="False"/>
        <field name="search_view_id" ref="sea_hr_attendance_view_search"/>
    </record>

    <record id="sea_hr_attendance_calendar_action" model="ir.actions.act_window">
        <field name="name">Attendances Calendar</field>
        <field name="res_model">sea.hr.attendance</field>
        <field name="view_mode">tree,form</field>
        <field name="domain">[('company_id','in',company_ids)]</field>
        <field name="view_id" eval="False"/>
        <field name="search_view_id" ref="sea_hr_attendance_view_search"/>
    </record>

    <record id="sea_hr_attendance_action_view_tree" model="ir.ui.view">
        <field name="name">sea.hr.attendance.tree</field>
        <field name="model">sea.hr.attendance</field>
        <field name="arch" type="xml">
            <tree>
                <field name="company_id" readonly="0"/>
                <field name="date" invisible="0"/>
                <field name="month"/>
                <field name="job_code_compute" invisible="1"/>
                <field name="year"/>
            </tree>
        </field>
    </record>

<?xml version="1.0" encoding="utf-8"?>
<odoo>
	<data>
		<record id="invoice_vendor_data_accounting_form" model="ir.ui.view">
			<field name="name">invoice.vendor.data.accounting.form</field>
			<field name="model">account.invoice</field>
			<field name="inherit_id" ref="account.invoice_supplier_form" />
			<field name="arch" type="xml">
				<xpath expr="/form/sheet/notebook/page[1]" position="after">
                    <page string="Invoice Info">
                        <h2 style="color: #7C7BAD;">Invoice Information</h2>
						<group>
							<group>
								<field name="symbol_invoice"/>
								<field name="supplier_invoice_number"/>
								<field name="supplier_invoice_date"/>
								<field name="supplier_vat"/>
							</group>
						</group>
                    </page>
                </xpath>
			</field>
		</record>
	</data>
</odoo>
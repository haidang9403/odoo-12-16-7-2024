<odoo>
    <!-- Inherited Stove move line tree view -->
    <record id="sea_invoice_consolidation_inherit" model="ir.ui.view">
        <field name="name">sea_invoice_consolidation_tree_inherit</field>
        <field name="model">account.invoice</field>
        <field name="inherit_id" ref="account.invoice_tree"/>
        <field name="arch" type="xml">
            <field name='partner_id' position='after'>
                <field name="sea_view_choose_to_invoice_consolidation"/>
                <field name="sea_consolidation_state"/>
            </field>
            <field name='origin' position='after'>
                <field name="sea_bill_pos"/>
            </field>
        </field>
    </record>

    <record id="invoice_form_inherit_po_code" model="ir.ui.view">
        <field name="name">invoice.form.inherit.po.code</field>
        <field name="model">account.invoice</field>
        <field name="inherit_id" ref="account.invoice_form"/>
        <field name="arch" type="xml">
            <field name='partner_bank_id' position='after'>
                <field name="sea_customer_po_name" invisible="1"/>
                <field name="sea_customer_po_code" invisible="1"/>
            </field>
        </field>
    </record>
</odoo>